{
  "name": "plex-analyzer",
  "version": "1.0.0",
  "description": "Plex Library Analyzer with Angular frontend and Express backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch backend/src/app.ts",
    "dev:frontend": "ng serve --proxy-config frontend/proxy.conf.json",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:frontend": "ng build",
    "start": "node backend/dist/app.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest --config backend/jest.config.js",
    "test:frontend": "ng test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "eslint backend/src/**/*.ts",
    "lint:frontend": "ng lint",
    "typecheck": "npm run typecheck:backend && npm run typecheck:frontend",
    "typecheck:backend": "tsc --noEmit -p backend/tsconfig.json",
    "typecheck:frontend": "tsc --noEmit -p frontend/tsconfig.json",
    "ng": "ng"
  },
  "keywords": [
    "plex",
    "analyzer",
    "angular",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "axios": "^1.7.7",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "ng2-charts": "^5.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-eslint/builder": "^17.5.2",
    "@angular-eslint/eslint-plugin": "^17.5.2",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular-eslint/schematics": "^17.5.2",
    "@angular-eslint/template-parser": "^17.5.2",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.0",
    "@eslint/js": "^9.30.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "~5.4.2",
    "typescript-eslint": "^8.35.0"
  }
}